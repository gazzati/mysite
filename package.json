{
  "name": "mysite",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/gazzati/mysite.git",
  "author": "t.gazzaev <t.gazzaev@sftpro.ru>",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --progress --mode development",
    "build": "yarn clean && webpack --progress --mode production",
    "type": "tsc --project tsconfig.json --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts --fix",
    "clean": "rm -rf ./build"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@gazzati/eslint-config-node": "^1.0.7",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "html-webpack-plugin": "^5.5.0",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.3",
    "sass-loader": "^13.2.0",
    "simple-pre-commit": "^1.2.1",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "simple-pre-commit": "npx lint-staged",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
